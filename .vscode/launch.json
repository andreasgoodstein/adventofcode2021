{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-read=./",
        "--allow-net",
        "runner.ts"
      ],
      "port": 0,
      "outputCapture": "std"
    }
  ]
}
